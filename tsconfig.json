{
    "compilerOptions": {
        "module": "node16",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "typeRoots": ["./types"],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react"
    }
}
